  "editor.codeLensFontFamily": "'Operator Mono', 'Operator Mono Light', 'OperatorMono-Light'",
  "editor.codeLensFontSize": 14,
  "editor.tokenColorCustomizations": {
    // Theme name (ie Visual Studio Code Dark)
    "[One Dark Pro]": {
      "textMateRules": [
        {
          "name": "HTML Brackets",
          "scope": [
            "punctuation.definition.tag.begin.html",
            "meta.tag.structure.head.end.html",
            "punctuation.definition.tag.end.html",
            "meta.tag.structure.head.end.html"
          ],
          "settings": {
            "foreground": "#ffcc00"
          }
        },
        {
          "name": "JSX Brackets",
          "scope": ["punctuation.definition.tag.jsx"],
          "settings": {
            "foreground": "#ffcc00"
          }
        }
      ]
    }
  },
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "editor.codeActionsOnSave": {},
  "editor.formatOnPaste": true,
  "editor.foldingMaximumRegions": 6000,

  "security.workspace.trust.untrustedFiles": "open",

  "debug.console.fontFamily": "'Operator Mono', 'Operator Mono Light', 'OperatorMono-Light'",
  "debug.console.fontSize": 14,

  "terminal.integrated.fontFamily": "'Operator Mono', 'Operator Mono Light'",
  "terminal.integrated.cursorStyle": "line",
  "terminal.integrated.fontSize": 14,
  "terminal.external.windowsExec": "C:\\Users\\xoxo\\AppData\\Local\\Microsoft\\WindowsApps\\wt.exe",
  "terminal.explorerKind": "external",

  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "explorer.compactFolders": false,
  "explorer.fileNesting.enabled": true,

  "git.autofetch": true,
  "git.enableSmartCommit": true,
  "git.confirmSync": false,

  // EXTENSIONS

  // Auto Rename Tag
  "auto-rename-tag.activationOnLanguage": ["*"],

  // Brackets Pair Colorizer
