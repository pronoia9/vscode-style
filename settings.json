  "window.zoomLevel": 1, // 0.5 for macbook, 0 for windows

  "workbench.colorCustomizations": {},
  "workbench.colorTheme": "One Dark Pro",
  "workbench.preferredLightColorTheme": "One Dark Pro",
  "workbench.preferredHighContrastColorTheme": "One Dark Pro",
  "workbench.iconTheme": "vscode-icons",
  "workbench.editor.decorations.colors": true,
  "workbench.preferredDarkColorTheme": "One Dark Pro",
  "workbench.tree.indent": 16,
  "workbench.tree.renderIndentGuides": "always",

  "editor.cursorSmoothCaretAnimation": true,
  "editor.tabSize": 2,
  "editor.cursorBlinking": "smooth",
  "editor.fontLigatures": true,
  "editor.formatOnType": true,
  "editor.wordWrap": "on",
  "editor.fontFamily": "'Operator Mono', 'OperatorMono Nerd Font', 'Operator Mono Light','OperatorMono-Light'",
  "editor.fontSize": 14,
  "editor.inlineHints.fontFamily": "'Operator Mono', 'OperatorMono Nerd Font', 'Operator Mono Light', 'OperatorMono-Light'",
